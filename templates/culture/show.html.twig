{% extends 'base_front.html.twig' %}

{% block title %}Culture{% endblock %}

{% block body %}
    <h1>Culture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ culture.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ culture.nom }}</td>
            </tr>
            <tr>
                <th>Surface</th>
                <td>{{ culture.surface }}</td>
            </tr>
            <tr>
                <th>Date_plantation</th>
                <td>{{ culture.datePlantation ? culture.datePlantation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_recolte</th>
                <td>{{ culture.dateRecolte ? culture.dateRecolte|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ culture.region }}</td>
            </tr>
            <tr>
                <th>Type_culture</th>
                <td>{{ culture.typeCulture }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_culture_index') }}">back to list</a>

    <a href="{{ path('app_culture_edit', {'id': culture.id}) }}">edit</a>

    {{ include('culture/_delete_form.html.twig') }}
{% endblock %}
