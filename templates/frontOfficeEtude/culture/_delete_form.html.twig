<form method="post" action="{{ path('app_culture_delete', {'context': app.request.get('context'),'id': culture.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ culture.id) }}">
    <button class="button">Delete</button>
</form>